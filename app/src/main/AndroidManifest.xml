<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="snow.app.ideelee">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RateServiceProviderActivity"></activity>
        <activity android:name=".PaymentSuccessfulActivity" />
        <activity android:name=".PaymentDetailsActivity" />

        <activity android:name=".fooddelivery.restdetails.RestDetailsActivity"></activity>
        <!-- <activity android:name=".fooddelivery.RestaurantsList"></activity> -->
        <activity
            android:name=".HomeScreen.help.HelpActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".HomeScreen.orders.OrderDetails.OrderDetailActivity" />
        <activity
            android:name=".forgot.ForgotPassword"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".HomeScreen.orders.MyOrders" />
        <activity android:name=".BookingAppointment" />
        <activity
            android:name=".HomeScreen.HomeNavigation"
            android:label="@string/title_activity_home_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".OTP"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".Register"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".Login"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeScreen.ServiceActivity" />
        <activity android:name=".HomeScreen.invites.InviteActivity" />
        <activity android:name=".BookingActivity"/>
    </application>

</manifest>